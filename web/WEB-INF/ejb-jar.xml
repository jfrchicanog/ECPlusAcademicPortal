<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        version="3.2"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">

    <enterprise-beans>
        <session>
            <ejb-name>Edicion</ejb-name>
            <ejb-class>es.uma.ecplusproject.ejb.Edicion</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <local>es.uma.ecplusproject.ejb.EdicionLocal</local>
            <env-entry>
                <env-entry-name>ecplus.resources.FILES_DIR</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/var/ecplus-ap/hashes</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>
</ejb-jar>
